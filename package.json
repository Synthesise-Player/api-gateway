{
  "name": "starter-service",
  "version": "1.0.0",
  "description": "A starter microservice",
  "private": true,
  "engines": {
    "node": "^10.12.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "start:cluster": "sl-run server/server.js",
