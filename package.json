    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^5.0.2",
    "cucumber-pretty": "^1.4.1",
    "istanbul": "^0.4.5",
    "morgan": "^1.9.0",
    "supertest": "^3.1.0"
  }
